{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": "off",
    "no-multiple-empty-lines": "error",
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "no-underscore-dangle": "off"
  }
}
